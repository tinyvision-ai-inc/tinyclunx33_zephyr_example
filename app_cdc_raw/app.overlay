#include <riscv/tinyvision/add_i2c0_pll.dtsi>

/* USB interfaces */

&zephyr_udc0 {
	num-in-endpoints = <3>;
	num-out-endpoints = <2>;
	status = "okay";

	cdc0: zephyr_cdc0 {
		compatible = "zephyr,cdc-raw";
	};
};
